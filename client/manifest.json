{
    "name": "Carbon Footprint Calculator",
    "description": "Your own Carbon Footprint Calculator",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./obj-16x16.png",
        "32": "./obj-32x32.png",
        "48": "./obj-48x48.png"
    },
    "background": {
        "scripts": [
            "./background.js"
        ]
    },
    "options_page": "./options.html",
    "browser_action": {},
    "permissions": [
        "tabs",
        "storage",
        "http://localhost:3000/",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}